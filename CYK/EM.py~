import random
from random import choice 


coins = ['coin1', 'coin2'] 

outCome = ['H', 'T']
count = {}

i = 0 
while i < 10:
    choice = random.choice(coins) 
    count[choice] = count.setdefault(choice,0) + 1
    j = 0
    while j < 10: 
        #print '\t'+str(
        #   random.choice(outCome))
        j += 1
    i += 1

print count




